import java.util.Scanner;
public class Main

{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	     double r,h;
        r = sc.nextDouble();
        h = sc.nextDouble();
        double vol = (3.14)*(r*r)*h;
        System.out.println("Volume of the Cylinder:" + vol);


	}
}
