import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	    int sq;
	    boolean ps=false;
	    for(int i=1;i*i<=a;i++){
	        sq=a/i;//5
	        if(sq==i){
	            ps=true;
	            break;
	        }
	        
	    }
	    if(ps){
	        System.out.print("Perfect Square");
	    }
	    else{
	        System.out.print("Not a perfect square");
	        
	    }
	
	}
}
