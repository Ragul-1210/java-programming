import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double n = sc.nextDouble();
        double sum=0;
        for(int i=1;i<=10;i++){
            sum += Math.pow(n, i) / i;
        }
        System.out.print(sum+" ");
        
    }
}
