import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	    int prod=1;
	    while(a!=0){
	        int digit=a%10;
	        prod=prod*digit;
	        a/=10;
	    }
	    System.out.print(prod);
	    }
}
