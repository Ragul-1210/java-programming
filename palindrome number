import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	    int n=a;
	    int rev=0;
	    if(a==0){
	        System.out.print("1");
	        return;
	    }
	    while(a!=0){
	        int digit=a%10;//3
	        rev=rev*10+digit;
	        a/=10;
	    }
	    if(rev==n){
	        System.out.print("It is palindrome");
	    }
	    else{
	        System.out.print("It is not a palindrome");
	    }
	}
}
