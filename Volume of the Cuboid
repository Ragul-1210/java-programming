import java.util.Scanner;
public class Main

{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	      int l,b,h;
        l = sc.nextInt();
        b = sc.nextInt();
        h = sc.nextInt();
        int vol = l*b*h;
        System.out.println("Volume of the Cuboid:" + vol);

	}
}
