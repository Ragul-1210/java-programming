import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        char ch=sc.next().charAt(0);
        if(ch=='r'){
            System.out.print("Stop");
        }
        else if(ch=='y'){
            System.out.print("Ready");
        }
        else if(ch=='g'){
            System.out.print("Go");
        }
        else{
            System.out.print("Invalid input");
        }
    }
}
