import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        //int n = sc.nextInt();
        int sum=1,term=1;
        System.out.print(term+" ");
        for(int i=2;i<=10;i++){
            sum+=term;
            System.out.print(sum+" ");
            term++;
        }
    }
}
