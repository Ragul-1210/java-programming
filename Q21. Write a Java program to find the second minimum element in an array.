import java.util.Arrays;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        if (n < 2) {
            return;
        }
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
        }
        Arrays.sort(arr);
        int secondMin = arr[0];
        for (int i = 1; i < n; i++) {
            if (arr[i] != arr[0]) {
                secondMin = arr[i];
                break;
            }
        }
        System.out.println("The second minimum element is: " + secondMin);
        sc.close();
    }
}
